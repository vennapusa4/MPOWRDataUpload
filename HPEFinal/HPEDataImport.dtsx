            UnZipFiles();

		}

		void UnZipFiles()
		{
			try
			{
                OleDbDataAdapter adapter = new OleDbDataAdapter();
                DataTable Masterdt = new DataTable();

                adapter.Fill(Masterdt, Dts.Variables["User::Files"].Value);
                

                string path = Dts.Variables["$Package::BlobDownloadPath"].Value.ToString();
				string[] zipFileArray = Directory.GetFiles(path);
				string[] extensions = Dts.Variables["$Package::FileExtensions"].Value.ToString().Split(',');
                DataTable dt = new DataTable();
                dt.Columns.Add("FileName", typeof(string));
                dt.Columns.Add("ParentFileName", typeof(string));
                dt.Columns.Add("FileId", typeof(int));
              //  dt.Columns.Add("RegionId", typeof(int));
                dt.Columns.Add("Geos", typeof(string));
                dt.Columns.Add("Countries", typeof(string));
                dt.Columns.Add("Years", typeof(string));
                dt.Columns.Add("DataLevel", typeof(string));
                dt.Columns.Add("UploadedBy", typeof(string));
                dt.Columns.Add("ModifiedBy", typeof(string));
                foreach (string file in zipFileArray)
				{
                  
                    string extension = Path.GetExtension(file);
					if (extensions.Contains<string>(extension))
                    {
                        var Parentfile = (from DataRow Ro in Masterdt.Rows
                                        where file.Contains(Ro["FileName"].ToString())
                                        select Ro).FirstOrDefault();
                        string exeName = Dts.Variables["$Package::PathTo7zip"].Value.ToString();
						ProcessStartInfo pro = new ProcessStartInfo();
						pro.WindowStyle = ProcessWindowStyle.Hidden;
						pro.FileName = exeName;
						pro.Arguments = string.Format("x \"{0}\" -y -o\"{1}\"", file, path + "\\");
						Process x = Process.Start(pro);
						x.WaitForExit();
                        var fileName= file.Substring(Dts.Variables["$Package::BlobDownloadPath"].Value.ToString().Length + 1);
                        File.Copy(file, Dts.Variables["$Package::ArchivalPath"].Value.ToString()+ "\\"+fileName,true);
                        File.Delete(file);
                        string[] zipFileArrayRegion= Directory.GetFiles(path);
                        foreach (var csv in zipFileArrayRegion)
                        {
                            var exists = (from DataRow Ro in dt.Rows
                                          where Ro["FileName"].ToString() == csv.Substring(Dts.Variables["$Package::BlobDownloadPath"].Value.ToString().Length + 1)
                                          select Ro).ToList().Count();
                            DataRow dr = dt.NewRow();
                            if (Path.GetExtension(csv)==".csv" && exists==0)
                            {
                                dr[0] = csv.Substring(Dts.Variables["$Package::BlobDownloadPath"].Value.ToString().Length + 1);
                                dr[1] = file;
                                dr[2] = Parentfile["ID"];
                                //dr[3] = Parentfile["RegionID"];
                                dr[3] = Parentfile["Geos"];
                                dr[4] = Parentfile["Countries"];
                                dr[5] = Parentfile["Years"];
                                dr[6] = Parentfile["DataLevel"];
                                dr[7] = Parentfile["UploadedBy"];
                                dr[8] = Parentfile["ModifiedBy"];
                                dt.Rows.Add(dr);
                            }
                        
                        }


                    }
				}
                DataTable dtemails1 = dt.DefaultView.ToTable(true, "UploadedBy");
                string strTo = string.Empty;
                foreach (DataRow item in dtemails1.Rows)
                {
                    strTo = string.Join(";", item["UploadedBy"].ToString());
                }
                var allmails = strTo.Trim(';');
                DataTable dtemails2 = dt.DefaultView.ToTable(true, "ModifiedBy");
               
                foreach (DataRow item in dtemails2.Rows)
                {
                    if (allmails.IndexOf(item["ModifiedBy"].ToString())==-1)
                    {
                        strTo += ";"+ string.Join(";", item["ModifiedBy"].ToString());
                    }
                  
                }


                Dts.Variables["User::ChildFiles"].Value = dt;
                Dts.Variables["User::AllEmails"].Value = strTo;

                // delete the blob file
                CloudStorageAccount storageAccount = CloudStorageAccount.Parse(Dts.Variables["$Package::Connection"].Value.ToString());
                CloudBlobClient blobClient = storageAccount.CreateCloudBlobClient();
                CloudBlobContainer container = blobClient.GetContainerReference(Dts.Variables["$Package::BlobContainer"].Value.ToString());

                if (container.Exists())
                {
                    foreach (DataRow item in Masterdt.Rows)
                    {
                        string extension = Path.GetExtension(item["FileName"].ToString());
                        if (extensions.Contains<string>(extension))
                        {
                            CloudBlockBlob blockBlob = container.GetBlockBlobReference(item[1].ToString());
                            if (container.GetBlockBlobReference(item["FileName"].ToString()).Exists())
                            {
                                blockBlob.Delete();
                            }
                        }
                    }
                }
                Dts.TaskResult = (int)ScriptResults.Success;

			}
			catch (Exception ex)
			{
                Dts.Events.FireError(1, ex.TargetSite.ToString(), ex.Message.ToString(), "", 0);
                using (StreamWriter sw = File.CreateText(Dts.Variables["$Package::BlobDownloadPath"].Value + "\\" + Dts.Variables["$Package::ValidationLogFile"].Value.ToString()))
                {
                    sw.Write(ex.Message.ToString() + "-" + ex.InnerException.Message.ToString());
                    
                    sw.WriteLine();
                    sw.Close();
                }
                Dts.TaskResult = (int)ScriptResults.Failure;
			}
		}


